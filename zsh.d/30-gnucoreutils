which gcp > /dev/null 

if [ $? -eq 0 ]; then
    if [ -f /etc/zsh/gdircolors ]; then
        eval `gdircolors /etc/zsh/gdircolors`
    else
        eval `gdircolors`
    fi

    #alias ls='gls --color -F'
    alias ll='ls -l'
    alias la='ls -a'
    alias cp='gcp'
    alias mv='gmv'
    alias rm='grm'
    alias du='gdu'
    alias df='gdf -a'
fi
